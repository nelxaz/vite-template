{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "@vue/prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2015
  },
  "plugins": ["vue"],
  "rules": {
    "no-unused-vars": "off",
    "vue/no-unused-vars": "warn"
  }
}
